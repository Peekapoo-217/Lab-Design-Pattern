package com.example.main;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        ConfigProvider provider = new FileConfigProvider(); // Hoáº·c new DatabaseConfigProvider()

        ConfigurationManager configManager = ConfigurationManager.getInstance(provider);

        System.out.println("Database URL: " + configManager.getConfig("database_url"));
        System.out.println("Server Port: " + configManager.getConfig("server_port"));
	}	

}
